{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "functions": {
    "dist/main.js": {
      "maxDuration": 60
    }
  }
}